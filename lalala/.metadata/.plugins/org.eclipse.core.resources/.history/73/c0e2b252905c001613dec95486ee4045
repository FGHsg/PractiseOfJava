import java.awt.*;
import javax.swing.*;
public class GUI1_1 extends JFrame{
	JButton a,b,c,d,e;
	public static void main(String[] args){
		GUI1_1 a = new GUI1_1();
		//JFrame a = new JFrame();
		
	}
	
	public GUI1_1(){
		a = new JButton("东");
		b = new JButton("西");
		c = new JButton("南");
		d = new JButton("北");
		e = new JButton("中");
		this.setTitle("lily");
		this.setSize(350, 280);
		this.setLocation(380,250);
		 
		this.add(a,BorderLayout.EAST);
		this.add(a,BorderLayout.WEST);
		this.add(a,BorderLayout.SOUTH);
		this.add(a,BorderLayout.NORTH);
		this.add(a,BorderLayout.CENTER);
		this.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		this.setVisible(true);
		
	}
}
