
public class TestReflection {

	public static void main(String[] args) {
		String str = "T";
		
		Class.forName(srr);
	}

}

class T{
	int i;
	String s;
	public void mm(){
		System.out.println("mm invoked!");
	}
	
	
	public int getI() {
		return i;
	}
	public void setI(int i) {
		this.i = i;
	}
	public String getS() {
		return s;
	}
	public void setS(String s) {
		this.s = s;
	}
	
	
}