		cfs.put("_id", "id");
		cfs.put("_name", "name");
		cfs.put("_age", "age");//yuanben zai peizhiwenjianzhong 
		
	}
	public void save(Student s) {	
		
		
		String sql =  createSQL();
	}

	private String createSQL() {
		String str1;
		String str2;
		for(int i =0;i<cfs.size();i++){
			str2+="?,";
		}
		
		String sql = "insert into" + tableName +"(" = str1 + ")" +"values (" +str2 + ")";
		
		return null;
	}

}
