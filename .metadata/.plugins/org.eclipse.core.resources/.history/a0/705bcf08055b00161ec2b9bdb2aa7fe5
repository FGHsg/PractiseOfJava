import java.util.*;
public class Seven_1 {
	public static void main(String[] args){
		Scanner in = new Scanner(System.in);
		try{
		int a= in.nextInt();
		 int b= in.nextInt();
		 int c =a/b;
		  }catch(exception e){
			  System.out.println("输入类型不符");
		  }
		 System.out.println(c);
		 //show1.show();
	}
}

class show1{
	static void show(){
	System.out.println("程序顺利运行中");
	}
}